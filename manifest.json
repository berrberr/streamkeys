{
  "manifest_version": 2,
  "name": "Grooveshark/Bandcamp Hotkeys With Media Keys",
  "description": "Global hotkeys for grooveshark and bandcamp",
  "version": "0.9.5",
  "icons": {"48": "icon48.png", "128": "icon128.png"},
  "options_page": "options.html",
  "background": {
    "scripts": [
      "lib/jquery-1.10.2.js",
      "background/background.js"
    ]
  },
  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "js": [
      "lib/jquery-1.10.2.js",
      "contentscript/contentscript.js"
    ]
  }],
  "web_accessible_resources": ["lib/jquery-1.10.2.js"],
  "permissions": ["tabs", "<all_urls>", "storage"]
}