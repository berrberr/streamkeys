{
  "env": {
    "browser": true,
    "es6": true,
    "jasmine": true,
    "jquery": true,
    "node": true
  },
  "extends": [
    "plugin:jasmine-jquery/recommended",
    "plugin:prettier/recommended"
  ],
  "globals": {
    "$": true,
    "console": true,
    "chrome": true,
    "require": true,
    "define": true,
    "module": true,
    "$data": true
  },
  "rules": {
    "eol-last": ["error", "always"],
    "linebreak-style": ["error", "unix"],
    "no-console": ["error", { "allow": ["log", "warn", "error"] }],
    "no-trailing-spaces": ["error"],
    "no-undef": "error",
    "no-unused-vars": "error",
    "strict": ["error", "global"]
  }
}
